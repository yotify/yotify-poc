<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:model="clr-namespace:Yotify.Data.Model.Playlist"
             x:Class="Yotify.MainPage">
    <CollectionView>
        <CollectionView.ItemsSource>
            <x:Array Type="{x:Type model:IPlaylist}">
                <model:YoutubePlaylist Name="Playlist 1"/>
                <model:YoutubePlaylist Name="Playlist 2"/>
                <model:YoutubePlaylist Name="Playlist 3"/>
            </x:Array>
        </CollectionView.ItemsSource>
    </CollectionView>
</ContentPage>
